<!DOCTYPE html>
<html>
<head>
  <title>Visit Quadra</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <!--added for troubleshooting-->
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDkhnN543M2yQwFVs1x_WjyYvXNXbGhaws">
</script>

</head>
<body>
	<%= render "layouts/header" %>
	
	<%= flash.each do |type, message| %>
		<div class="alert alert-info fade-in">
			<button class="close" data-dismiss="alert">x</button>
			<%= message %>
		</div>
	<% end %>
	
	<div class="container">
	<%= yield %>
	</div>


<!--script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDkhnN543M2yQwFVs1x_WjyYvXNXbGhaws&callback=initMap()">
    </script-->

</body>
</html>
